.tabs .title {
  display: flex;
}
.tabs .title .title_name {
  flex: 1;
  border: #6DBBB4 4rpx solid;
  margin: 0 10rpx;
  border-radius: 10rpx;
  text-align: center;
  font-size: 34rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: #6DBBB4;
}
.tabs .title .active {
  background-color: #6DBBB4;
  color: white;
}
