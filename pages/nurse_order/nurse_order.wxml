<view class="headTitle" style="height:{{nav}}px;margin-top: {{navHeight - nav}}px">
    <view class="img">
        <image src="../../img/QR-code.png" mode="widthFix" bindtap="handleQRCode"/>
    </view>
    <view class='headTitle_title' bindtap="showChangeUser">预约护士</view>
</view>
<van-popup
    show="{{ showUser }}"
    position="bottom"
    custom-style="height: 20%;"
    bind:close="onClose"
    closeable
    round>
    <view class="userList">
        <view class="list_item" wx:for="{{userList}}" wx:key="*this" bindtap="handleChangeUser" data-userItem="{{item}}">
            {{item.medicalCard === 's10001' ? '管理' : item.medicalCard === 'n10002' ? '预约' : 
            item.medicalCard === 'n10003' ? '注射' : item.medicalCard === 't10004' ? '检查' :''}}
        </view>
        <view class="add_item">
            <navigator url="../super_user/super_user" hover-class="navigator-hover" open-type="navigate">
                <image src="../../img/addUser.png" mode="widthFix"/>
            </navigator>
        </view>
    </view>
</van-popup>



<view class="top_Title">
    <view class="top_Title_search">
        <van-search placeholder="请输入搜索关键词" bind:change='handleSearch' />
    </view>
    <view class='top_Title_icon'>
        <text class='iconfont icon-genghuan' bindtap='handleClickGenghuan'></text>
        <text>|</text>
        <text class='iconfont icon-shaixuan' bindtap='handleClickShaiXuan'></text>
    </view>

    <van-popup
        show="{{ genghuanShow }}"
        bind:close="onCloseGengHuan"
        custom-class='genghuan'>
        <view class='container'>
            <view class='container_item'><navigator url="../addProject/addProject" open-type="navigate">创建项目</navigator></view>
            <view class='container_item'><navigator url="../set_order/set_order" open-type="navigate">管理项目</navigator></view>
            <view class='container_item'><navigator url="../my_project/my_project" open-type="navigate">更改项目</navigator></view>
            <view class='container_item'><navigator url="../user_exitOrder/user_exitOrder" open-type="navigate">用户退约</navigator></view>
        </view>
    </van-popup>
    <van-popup
        show="{{ shaixuanShow }}"
        position="bottom"
        custom-style="height: 50%;"
        bind:close="onCloseShaixaun"
        closeOnClickOverlay="{{false}}"
        custom-class='shaixuan'>
        <view class='select_project'>
            <view class='select_project_title'>项目名称</view>
            <view class='select_project_input'><input type="text" bindinput="getUserInput" value='{{selectProject}}'/></view>
        </view>
        <view class="select_date" bindtap='showDateSelect'>
            <view class='select_date_title'>日期</view>
            <view class="select_date_container">
                {{date ? date : '选择日期'}} <text class="iconfont icon-riqi"></text>
            </view>
        </view>

        <view class='select_state'>
            <view class='select_state_title'>状态</view>
            <view class='select_state_list'>
                <view class='select_state_list_item' style='{{item.isActive ? "background-color: #6dbbbc; color: white;" : ""}}' wx:for="{{selectCondition}}" :key='*this' bindtap='clickItem' data-item='{{item}}'>{{item.value}}</view>
            </view>
        </view>
        <view class='submit'>
            <view class='submit_confirm' bindtap='submit'>确认</view>
        </view>
    </van-popup>
    <van-calendar
        show="{{ showDateSelect }}"
        bind:close="onCloseDate"
        min-date='{{minDate}}'
        color= '#6dbbbc'
        bind:confirm="onConfirmDate"/>

</view>

<view class='tabs'>
    <view class='tabs_item'>
        <view wx:if="{{userInfo.length}}">
            <view class="item" wx:for="{{userInfo}}" wx:key="*this" data-index='{{index}}'>
            <navigator url="../nurse_apply/nurse_apply?medicalCard={{item.medicalCard}}&id={{item.id}}&status={{item.status}}&selectIndex={{selectIndex}}" open-type="navigate">
                <view class="top">
                <view class="top_img">
                    {{index + 1}}
                </view>
                <view class="top_title">
                    <view class="title_address">中南大学湘雅二医院</view>
                    <view class="title_project">{{item.orderProject}}</view>
                </view>
                <view class="top_icon">
                    <image mode="widthFix" src="../../img/enterInto.png"/>
                </view>
            </view>
            <view class="bottom">
                <view class="btm_number">卡号:{{item.medicalCard}}</view>
                <view class="btm_name">姓名:{{item.name}}</view>
            </view>
            </navigator>
        </view>
        </view>
        <view wx:if="{{!userInfo.length}}" class='noData'>
            <image src='../../img/NoData.png' mode="widthFix"/>
        </view>
    </view>

