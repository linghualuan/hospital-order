page,
view,
image,
text,
swiper,
swiper-item,
navigator,
button {
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}
.header {
  height: 70rpx;
  padding: 0 20rpx;
  position: relative;
}
.header .header_img {
  height: 70rpx;
  width: 70rpx;
  position: absolute;
}
.header .header_img image {
  width: 100%;
  height: 100%;
}
.header .header_inp {
  height: 100%;
}
.header .header_inp input {
  height: 100%;
  border-radius: 35rpx;
  border: 2rpx solid #ccc;
  padding-left: 70rpx;
}
.content {
  padding: 0 20rpx;
}
.content .item {
  width: 100%;
  height: 200rpx;
  background-color: #6DBBB4;
  margin: 30rpx 0;
  border-radius: 40rpx;
  padding: 10rpx;
}
.content .item navigator {
  position: relative;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 20rpx;
  border-radius: 40rpx;
}
.content .item navigator .left {
  flex: 4;
  display: flex;
  flex-direction: column;
}
.content .item navigator .left .left_top {
  flex: 5;
  display: flex;
  align-items: center;
}
.content .item navigator .left .left_top .item_img {
  flex: 1;
}
.content .item navigator .left .left_top .item_img image {
  display: block;
  width: 80%;
}
.content .item navigator .left .left_top .item_wrap {
  flex: 2;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.content .item navigator .left .left_top .item_wrap .inspect {
  flex: 1;
  color: white;
}
.content .item navigator .left .left_top .item_wrap .project {
  flex: 2;
  color: white;
  font-size: 50rpx;
}
.content .item navigator .right {
  flex: 1;
  position: relative;
}
.content .item navigator .right image {
  position: relative;
  left: 60rpx;
  top: 0;
  width: 50%;
  height: 50%;
}
.content .noData {
  width: 100vw;
  height: 100vw;
  margin: 0 auto;
}
.content .noData image {
  width: 100%;
  height: 100%;
}
