.project_wrap {
  width: 94%;
  height: 160rpx;
  background-color: #fff;
  border-radius: 20rpx;
  margin: 0 auto;
  margin-top: 20rpx;
  display: flex;
}
.project_wrap .project_left {
  height: 94rpx;
  padding: 10rpx;
  background-color: #6DBBB4;
  border-radius: 50%;
  margin: auto 0;
  margin-left: 60rpx;
}
.project_wrap .project_left .set {
  margin-top: 25rpx;
  height: 45rpx;
  width: 94rpx;
  text-align: center;
  border: 1px solid #fff;
  color: #fff;
}
.project_wrap .project_right {
  margin: auto 0;
  margin-left: 45rpx;
}
.project_wrap .project_right .check {
  height: 50rpx;
  font-size: 36rpx;
  font-weight: 600;
}
.project_wrap .project_right .project_name {
  width: 280rpx;
  font-size: 35rpx;
}
.project_wrap .project_icon {
  margin: auto 0;
  margin-left: 84rpx;
}
.project_wrap .project_icon image {
  width: 60rpx;
  height: 60rpx;
}
.bottom_wrap {
  display: flex;
  justify-content: space-between;
  width: 94%;
  height: 80rpx;
  background-color: #fff;
  margin: 0 auto;
  margin-top: 5rpx;
  border-radius: 20rpx;
}
.bottom_wrap .box1 {
  font-size: 28rpx;
  padding: 0 10rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  background-color: #dddddd;
  margin: auto 0;
  margin-left: 20rpx;
  border-radius: 15rpx;
}
.bottom_wrap .box2 {
  font-size: 28rpx;
  height: 50rpx;
  line-height: 50rpx;
  padding: 0 10rpx;
  text-align: center;
  border-radius: 15rpx;
  background-color: #dddddd;
  margin: auto 0;
}
.bottom_wrap .box3 {
  height: 50rpx;
  font-size: 28rpx;
  text-align: center;
  line-height: 50rpx;
  padding: 0 10rpx;
  border-radius: 15rpx;
  margin: auto 0;
  background-color: #dddddd;
  margin-right: 20rpx;
}
