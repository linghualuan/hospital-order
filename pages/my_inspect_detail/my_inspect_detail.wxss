page {
  background-color: #6DBBB4;
  padding: 30rpx;
}
page .content {
  position: relative;
}
page .content .left_div {
  width: 60rpx;
  height: 60rpx;
  background-color: #6dbbbc;
  position: absolute;
  top: 300rpx;
  left: -30rpx;
  z-index: 3;
  border-radius: 50%;
}
page .content .right_div {
  width: 60rpx;
  height: 60rpx;
  background-color: #6dbbbc;
  position: absolute;
  top: 300rpx;
  right: -30rpx;
  z-index: 3;
  border-radius: 50%;
}
page .content .title {
  display: flex;
  height: 60rpx;
}
page .content .title .title_img {
  flex: 1;
}
page .content .title .title_img image {
  width: 100%;
  height: 100%;
}
page .content .title .title_name {
  padding-left: 30rpx;
  flex: 13;
  color: white;
}
page .content .con_top {
  background-color: white;
  border-bottom: #6dbbbc 5rpx solid;
  font-size: 35rpx;
  display: flex;
  flex-direction: column;
  padding-top: 20rpx;
  border-radius: 20rpx;
  position: relative;
  padding-bottom: 40rpx;
  height: auto;
}
page .content .con_top .top_project {
  flex: 1;
  text-align: center;
  font-size: 50rpx;
  font-weight: 600;
}
page .content .con_top .top_date {
  flex: 1;
  color: #F26D07;
  display: flex;
  height: 80rpx;
}
page .content .con_top .top_date .item_date {
  flex: 1;
  line-height: 80rpx;
  text-align: right;
  padding-right: 40rpx;
}
page .content .con_top .top_date text {
  flex: 1;
  line-height: 80rpx;
  font-size: 50rpx;
}
page .content .con_top .top_date .textError {
  flex: 1;
  height: auto;
  padding-top: 20rpx;
  margin-right: 30rpx;
}
page .content .con_top .top_date1 {
  flex: 1;
  color: #F26D07;
  display: flex;
}
page .content .con_top .top_date1 .item_date {
  flex: 1;
  line-height: 80rpx;
  text-align: right;
  padding-right: 40rpx;
}
page .content .con_top .top_date1 .textError {
  flex: 1;
  height: auto;
  padding-top: 20rpx;
}
