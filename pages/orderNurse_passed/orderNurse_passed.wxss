page {
    background-color: #FAF2F9;
  }
  page .top_title {
    display: flex;
    align-items: center;
    background-color: white;
  }
  page .top_title .img {
    flex: 1;
    padding: 10rpx;
  }
  page .top_title .img image {
    width: 50%;
    text-align: center;
    margin-left: 30rpx;
  }
  page .top_title .title {
    flex: 6;
  }
  page .item {
    width: 94%;
    margin: 20rpx auto;
    height: 210rpx;
    background-color: white;
    display: flex;
    flex-direction: column;
    padding: 10rpx;
    border-radius: 20rpx;
  }
  page .item navigator {
    width: 100%;
    height: 100%;
  }
  page .item navigator .top {
    flex: 5;
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    border-bottom: 4rpx #FAF2F9 solid;
  }
  page .item navigator .top .top_img {
    flex: 1;
    border-radius: 50%;
  }
  page .item navigator .top .top_img image {
    width: 100%;
    border-radius: 50%;
  }
  page .item navigator .top .top_title {
    flex: 4;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
  }
  page .item navigator .top .top_title .title_address {
    flex: 1;
    margin: 10rpx 0;
  }
  page .item navigator .top .top_title .title_project {
    flex: 1;
    font-size: 35rpx;
    color: #999;
  }
  page .item navigator .top .top_icon {
    flex: 1;
  }
  page .item navigator .top .top_icon image {
    width: 100%;
  }
  page .item navigator .bottom {
    margin-top: 10rpx;
    flex: 2;
    justify-content: space-between;
    display: flex;
    text-align: center;
  }
  page .item navigator .bottom .btm_number {
    margin: 0 10rpx;
    flex: 1;
    height: 50rpx;
    border-radius: 25rpx;
    padding: 0 10rpx;
    font-size: 27rpx;
    line-height: 50rpx;
    background-color: #EEEEEE;
  }
  page .item navigator .bottom .btm_name {
    margin: 0 10rpx;
    flex: 1;
    height: 50rpx;
    border-radius: 25rpx;
    padding: 0 10rpx;
    font-size: 27rpx;
    line-height: 50rpx;
    background-color: #EEEEEE;
  }
  