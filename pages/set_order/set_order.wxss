page {
  padding-bottom: 30rpx;
}
page .header {
  height: 70rpx;
  padding: 0 20rpx;
  position: relative;
}
page .header .header_img {
  height: 70rpx;
  width: 70rpx;
  position: absolute;
}
page .header .header_img image {
  width: 100%;
  height: 100%;
}
page .header .header_inp {
  height: 100%;
}
page .header .header_inp input {
  height: 100%;
  border-radius: 35rpx;
  border: 2rpx solid #ccc;
  padding-left: 70rpx;
}
page .wrap .project .project_wrap {
  width: 94%;
  height: 160rpx;
  background-color: #6DBBB4;
  border-radius: 20rpx;
  margin: 0 auto;
  margin-top: 20rpx;
  display: flex;
}
page .wrap .project .project_wrap .project_left {
  height: 100rpx;
  padding: 10rpx;
  background-color: white;
  border-radius: 50%;
  margin: auto 0;
  margin-left: 60rpx;
}
page .wrap .project .project_wrap .project_left .set {
  margin-top: 26rpx;
  height: 45rpx;
  width: 100rpx;
  text-align: center;
  border: 1px solid #60a7a1;
  color: #61aaa4;
}
page .wrap .project .project_wrap .project_right {
  margin: auto 0;
  margin-left: 45rpx;
}
page .wrap .project .project_wrap .project_right .check {
  width: 180rpx;
  font-size: 30rpx;
  color: white;
  margin: 10rpx 0;
}
page .wrap .project .project_wrap .project_right .project_name {
  width: 280rpx;
  font-size: 35rpx;
  font-weight: 600;
  color: white;
}
page .wrap .project .project_wrap .project_icon {
  margin: auto 0;
  margin-left: 60rpx;
}
page .wrap .project .project_wrap .project_icon image {
  width: 60rpx;
  height: 60rpx;
}
page .noData {
  width: 100vw;
  height: 100vw;
  margin: 0 auto;
}
page .noData image {
  width: 100%;
  height: 100%;
}
