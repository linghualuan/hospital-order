page {
  background-color: #FAF2F9;
}
page .content {
  display: flex;
  flex-direction: column;
}
page .content .col {
  margin: 20rpx 40rpx;
  display: flex;
}
page .content .col .title {
  flex: 2;
  text-align: right;
  padding-right: 30rpx;
}
page .content .col .inp {
  flex: 5;
}
page .content .col .inp input {
  background-color: white;
  border-radius: 10rpx;
  padding-left: 30rpx;
}
page .content .demo {
  width: 400rpx;
  height: 200rpx;
  background-color: black;
}
