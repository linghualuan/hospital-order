page {
  background-color: #FAF2F9;
}
page .van-button--block {
  width: 100%;
}
page .headTitle {
  text-align: center;
  vertical-align: middle;
  display: flex;
  align-items: center;
  justify-content: center;
}
page .userList {
  padding: 30rpx;
}
page .userList .list_item {
  width: 120rpx;
  height: 120rpx;
  line-height: 120rpx;
  text-align: center;
  color: white;
  float: left;
  border-radius: 50%;
  margin-right: 20rpx;
  background-color: #6dbbbc;
}
page .userList .add_item {
  width: 120rpx;
  float: left;
}
page .userList .add_item image {
  width: 100%;
}
page .header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: white;
  width: 100vw;
}
page .header_search {
  flex: 7;
}
page .header_select {
  flex: 1;
  text-align: center;
}
page .header_select text {
  font-size: 40rpx;
}
page .shaixuan {
  padding: 20rpx;
  box-sizing: border-box;
  font-weight: 700;
}
page .shaixuan .select_project {
  margin-bottom: 20rpx;
}
page .shaixuan .select_project_title {
  margin: 10rpx 0;
}
page .shaixuan .select_project_input {
  width: 70%;
  background-color: #EEEEEE;
  border-radius: 10rpx;
}
page .shaixuan .select_project_input input {
  font-weight: 500;
  font-size: 30rpx;
  padding: 10rpx;
}
page .shaixuan .select_date {
  margin-bottom: 20rpx;
}
page .shaixuan .select_date_title {
  margin-bottom: 10rpx;
}
page .shaixuan .select_date_container {
  background-color: #eee;
  width: 70%;
  height: 70rpx;
  line-height: 70rpx;
  font-weight: 500;
  font-size: 30rpx;
  color: #555;
  padding: 0 20rpx;
  box-sizing: border-box;
}
page .shaixuan .select_date_container text {
  display: inline-block;
  float: right;
}
page .shaixuan .select_state {
  margin-bottom: 80rpx;
}
page .shaixuan .select_state_title {
  margin-bottom: 10rpx;
}
page .shaixuan .select_state_list {
  display: grid;
  grid-template-columns: 33% 33% 33%;
  grid-template-rows: 60rpx 60rpx;
  grid-column-gap: 30rpx;
  grid-row-gap: 20rpx;
}
page .shaixuan .select_state_list_item {
  font-weight: 500;
  font-size: 30rpx;
  line-height: 60rpx;
  text-align: center;
  background-color: #eee;
}
page .shaixuan .submit {
  width: 200rpx;
  height: 60rpx;
  margin: 0 auto;
  margin-bottom: 30rpx;
  border-radius: 10rpx;
  background-color: #6dbccc;
}
page .shaixuan .submit_confirm {
  text-align: center;
  color: white;
  line-height: 60rpx;
}
page .item {
  width: 94%;
  margin: 20rpx auto;
  height: 210rpx;
  background-color: white;
  display: flex;
  flex-direction: column;
  padding: 10rpx;
  border-radius: 20rpx;
}
page .item navigator {
  width: 100%;
  height: 100%;
}
page .item navigator .top {
  flex: 5;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  border-bottom: 4rpx #FAF2F9 solid;
}
page .item navigator .top .top_img {
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 60rpx;
  color: white;
  border-radius: 50%;
  text-align: center;
  margin-right: 30rpx;
  background-color: #6dbccc;
}
page .item navigator .top .top_img1 {
  flex: 1;
}
page .item navigator .top .top_img1 image {
  width: 100%;
  height: 100%;
}
page .item navigator .top .top_title {
  flex: 4;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding-left: 30rpx;
}
page .item navigator .top .top_title .title_address {
  flex: 1;
  margin: 10rpx 0;
}
page .item navigator .top .top_title .title_project {
  flex: 1;
  font-size: 35rpx;
  color: #999;
}
page .item navigator .top .top_icon {
  flex: 1;
}
page .item navigator .top .top_icon image {
  width: 100%;
}
page .item navigator .bottom {
  margin-top: 10rpx;
  flex: 2;
  justify-content: space-between;
  display: flex;
  text-align: center;
}
page .item navigator .bottom .btm_number {
  margin: 0 10rpx;
  flex: 1;
  height: 50rpx;
  border-radius: 25rpx;
  padding: 0 10rpx;
  font-size: 27rpx;
  line-height: 50rpx;
  background-color: #EEEEEE;
}
page .item navigator .bottom .btm_name {
  margin: 0 10rpx;
  flex: 1;
  height: 50rpx;
  border-radius: 25rpx;
  padding: 0 10rpx;
  font-size: 27rpx;
  line-height: 50rpx;
  background-color: #EEEEEE;
}
page .noData {
  width: 100vw;
  height: 100vw;
  margin: 0 auto;
}
page .noData image {
  width: 100%;
  height: 100%;
}
